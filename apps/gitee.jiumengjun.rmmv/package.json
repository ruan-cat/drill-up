{
	"name": "@ruan-cat/mv-docs",
	"version": "0.0.1",
	"description": "gitee.jiumengjun.rmmv 来自于别人提供的rmmv翻译源码",
	"main": "main.js",
	"type": "module",
	"private": true,
	"scripts": {
		"legacy:make-doc": "jsdoc2md --files ./rpg_*.js > ./dist/jsdoc2md-res.md",
		"legacy:make": "typedoc",
		"jsdoc:generate": "tsx scripts/index.ts",
		"docs:dev": "vitepress dev docs",
		"docs:build": "pnpm jsdoc:generate && vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:clean": "rimraf docs/jsdoc && rimraf docs/.vitepress/dist && rimraf docs/.vitepress/cache",
		"build": "pnpm docs:build"
	},
	"keywords": [],
	"author": "阮喵喵",
	"license": "ISC",
	"dependencies": {
		"typedoc": "^0.28.11",
		"typedoc-plugin-markdown": "^4.8.1",
		"vitepress-demo-plugin": "^1.4.7"
	},
	"devDependencies": {
		"@ruan-cat/vitepress-preset-config": "^1.3.0",
		"@types/node": "^20.19.11",
		"jsdoc-to-markdown": "^9.1.2",
		"tsx": "^4.20.5",
		"vitepress": "^1.6.4",
		"vue": "^3.5.20"
	}
}