		"jsdoc:generate": "tsx scripts/index.ts",
		"docs:dev": "vitepress dev docs",
		"docs:build": "npm run jsdoc:generate && vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:clean": "rimraf docs/jsdoc && rimraf docs/.vitepress/dist && rimraf docs/.vitepress/cache",
		"build": "npm run docs:build"
	},
	"keywords": [],
