{
	"name": "@ruan-cat/drill-up",
	"author": "阮喵喵",
	"description": "插件集合示例。阮喵喵改版。",
	"version": "3.6.2",
	"private": true,
	"type": "module",
	"main": "index.html",
	"scripts": {
		"dev": "vite",
		"dev:drill": "vite --mode drill",
		"dev:qj-en": "vite --mode qj-en",
		"vue-tsc": "vue-tsc -b",
		"tauri:init": "tauri init",
		"tauri:dev": "tauri dev",
		"tauri:build": "tauri build",
		"build:rpgmv-plugins": "tsup",
		"build:rpgmv-plugins:watch": "tsup --watch",
		"build:rm-ts-code-tsc": "tsc --project tsconfig.build-mv-code.json",
		"build:mv-ts-code-tsup": "tsup --watch",
		"build:drill": "vite build --mode drill",
		"build:qj-en": "vite build --mode qj-en",
		"build-muti": "cross-env isSingleDomain=true pnpm run -r \"/^build:.*/\" ",
		"build": "pnpm run build:qj-en",
		"postbuild": "tsx ./scripts/uglify.ts",
		"preview": "vite preview",
		"lint:eslint": "eslint . --fix",
		"format:prettier": "prettier --write . >logs/prettier-format-logs.md"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.2",
		"element-plus": "^2.11.1",
		"lodash-es": "^4.17.21",
		"vue": "^3.5.20"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^5.2.1",
		"@dotenvx/dotenvx": "^1.49.0",
		"@ruan-cat/vite-plugin-ts-alias": "^0.1.0",
		"@swc/core": "^1.13.5",
		"@tauri-apps/cli": "^2.8.3",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/fpsmeter": "^0.3.34",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.18.0",
		"@types/pixi.js": "4.5.4",
		"@types/uglify-js": "^3.17.5",
		"@vitejs/plugin-vue": "^6.0.1",
		"load-tsconfig": "^0.2.5",
		"lz-string": "^1.5.0",
		"pixi.js": "4.5.4",
		"sass-embedded": "^1.91.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"uglify-js": "^3.19.3",
		"unplugin-auto-import": "^20.0.0",
		"unplugin-vue-components": "^29.0.0",
		"upath": "^2.0.1",
		"vite": "^5.4.1",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-vue-devtools": "^8.0.1",
		"vue-tsc": "^3.0.6"
	}
}